    "sorting": { "default": {}},
    "tooltips": {
        "supportedTypes": {
            "default": true,
            "canvas": true
        },
        "roles": [
            "tooltips"
        ]
    },
    "dataViewMappings": [
        {
            "conditions": [{"dataValue": {"min": 1}}],
            "table": {
                "rows": {
                    "select": [{"for": {"in": "dataRows"}},{"for": {"in": "dataValue"}}]
                }
            }
        }
    ]
