                            "fontFamily": true
                        }
                    }
                },
                "fontSize": {
                    "displayName": "Text Size",
                    "description": "Size of the font in points (pt)",
                    "type": {
                        "formatting": {
                            "fontSize": true
                        }
                    }
                },
                "fontColor": {
                    "displayName": "Font Color",
                    "description": "The fill color of text value",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "fontBold": {
                    "displayName": "Bold",
                    "description": "display font in bold characters",
                    "type": {
                        "bool": true
                    }
                },
                "bgColor": {
                    "displayName": "Background Color",
                    "description": "The fill color of header background",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "alignment": {
                    "displayName": "Alignment",
